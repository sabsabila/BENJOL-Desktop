<basic:MyPage x:Class="TestWPPL.Dashboard.Dashboard"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TestWPPL.Dashboard"
      xmlns:lineChart="clr-namespace:Velacro.Chart.LineChart;assembly=Velacro"
      xmlns:basic="clr-namespace:Velacro.UIElements.Basic;assembly=Velacro"
      mc:Ignorable="d" 
      d:DesignHeight="365" d:DesignWidth="635"
      Title="Dashboard">
    <Grid Background="#FFF0F0F7">
        <Rectangle Fill="White" HorizontalAlignment="Left" Height="203" Margin="38,75,0,0" VerticalAlignment="Top" Width="554"/>
        <Image x:Name="bengkelPicture" HorizontalAlignment="Center" Height="159" Width="160" VerticalAlignment="Top" Margin="389,97,86,0" Source="/Image/camera-placeholder.jpg" />
        <Label Background="#FFF0F0F7" Content="Hi!" HorizontalAlignment="Left" Margin="52,83,0,0" VerticalAlignment="Top" Foreground="#FF636363" FontSize="24"/>
        <Label Content=":" HorizontalAlignment="Left" Margin="238,230,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="bengkelName" Text="name" HorizontalAlignment="Left" Margin="64,140,0,0" VerticalAlignment="Top" FontSize="20"/>
        <TextBlock x:Name="email" Text="email" HorizontalAlignment="Left" Margin="64,199,0,0" VerticalAlignment="Top" RenderTransformOrigin="3.81,2.562" FontSize="20"/>
        <TextBlock x:Name="telephone" Text="telphone" HorizontalAlignment="Left" Margin="64,172,0,0" VerticalAlignment="Top" FontSize="20"/>
        <TextBlock x:Name="address" Text="address" HorizontalAlignment="Left" Margin="64,226,0,0" VerticalAlignment="Top" FontSize="20"/>
    </Grid>
</basic:MyPage>
